<body class="stretched">
<div class="container">
</div>
    <!-- Document Wrapper
    ============================================= -->
    <div id="wrapper" class="clearfix">


        <!-- Header
        ============================================= -->
        <header id="header" class="transparent-header">

          <div id="header-wrap">

                    <!-- Primary Navigation
              ============================================= -->
              <nav class="navbar navbar-default navigation">
