</head>
<body class="stretched">

	<!-- Document Wrapper
	============================================= -->
	<div id="container" class="clearfix">

                   <!-- Primary Navigation
             ============================================= -->
            <% if @tour_page != "loaded" %>
             <nav class="navbar navbar-default navigation">
               <div class="container">
                 <!-- Brand and toggle get grouped for better mobile display -->
                 <div class="navbar-header">
                   <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                     <span class="sr-only">Toggle navigation</span>
                     <span class="icon-bar"></span>
                     <span class="icon-bar"></span>
                     <span class="icon-bar"></span>
                   </button>

                     <a class="navbar-brand" href="/">
                       <img class="logo" src="<%= asset_path 'acltc-logo-180.png'%>">
                     </a>

                       <!-- <a href="/" class="retina-logo" data-dark-logo="assets/theme/logo-dark@2x.png"><img src="assets/theme/logo@2x.png" alt="Canvas Logo"></a> -->
                 </div>
                 <!-- Collect the nav links, forms, and other content for toggling -->
       <%= render 'shared/navbar' %>

               </div><!-- /.container-fluid -->
             </nav>
            <% end %>




    <!-- #header end -->

    <% flash.each do |name, message| %>
      <div class="alert alert-<%= name %> alert-dismissible fade in" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">&times;</span><span class="sr-only">Close</span></button>
        <%= message %>
      </div>
    <% end %>

    <%= yield %>


    <%= render 'shared/inspectlet' %>
    <%= render 'shared/ga' %>
    <% if @tour_page != "loaded" %>
      <%= render 'shared/footer' %>
    <% end %>

  </div><!-- #wrapper end -->


